
<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewData["Title"]</title>
    <!-- font-awesome -->
    <link rel="stylesheet" href="/assets/css/font-awesome.min.css">
    <!-- Bootstrap 4.5.3 -->
    <link rel="stylesheet" href="/assets/bootstrap/css/bootstrap.min.css">
    <!-- CSS Implementing Plugins-->
    <link rel="stylesheet" href="/assets/css/plugins/apexcharts.css">
    <link rel="stylesheet" href="/assets/css/plugins/jquery.classycountdown.min.css">
    <link rel="stylesheet" href="/assets/css/plugins/nouislider.min.css">
    <link rel="stylesheet" href="/assets/css/plugins/sweetalert2.min.css">
    <link rel="stylesheet" href="/assets/css/plugins/select2.min.css">
    <link rel="stylesheet" href="/assets/css/plugins/swiper.min.css">
    <!-- CSS Template -->
    <link rel="stylesheet" href="/assets/css/theme.css">
    <!-- Here goes your custom.cs -->
    <link rel="stylesheet" href="/assets/css/custom.css">
    <!-- colors: amber,blue,browncyan,default,green,indigo,orange,pink,purple,red,teal,yellow -->
    <link rel="stylesheet" href="/assets/css/colors/red.css" >
</head>

<body>

    <div class="page-wrapper">

        <!-- Page Header -->
        <partial name="_Header" />
        <!-- header responsive -->
        <partial name="_ResponsiveHeader" />
        <!-- end header responsive -->
        <!-- end Page Header -->
        <!-- Page Content -->
        <main class="page-content">
            <div class="container">
                @RenderBody()
            </div>
        </main>

        <!-- end color switcher -->
        <!-- Page Loader -->
        <div class="page-loader">
            <div class="page-loader-content">
                <div class="logo-area">
                    <img src="/assets/images/logo.png" alt="loader">
                </div>
                <span class="loader"></span>
            </div>
        </div>
        <!-- end Page Loader -->

        <partial name="_Footer" />
        <!-- end Page Footer -->

    </div>



    <!-- JS Global Compulsory -->
    <script src="/assets/js/jquery-3.5.1.min.js"></script>
    <script src="/assets/js/plugins/popper.min.js"></script>
    <script src="/assets/bootstrap/js/bootstrap.min.js"></script>
    <!-- JS Impleenting Plugins -->
    <script src="/assets/js/plugins/apexcharts.min.js"></script>
    <script src="/assets/js/plugins/jquery.ez-plus.js"></script>
    <script src="/assets/js/plugins/jquery.knob.js"></script>
    <script src="/assets/js/plugins/jquery.throttle.js"></script>
    <script src="/assets/js/plugins/jquery.classycountdown.min.js"></script>
    <script src="/assets/js/plugins/jquery.nicescroll.min.js"></script>
    <script src="/assets/js/plugins/nouislider.min.js"></script>
    @*<script src="/assets/js/plugins/sweetalert2.all.min.js"></script>
        <script src="/assets/js/plugins/select2.full.min.js"></script>*@
    <script src="https://cdn.jsdelivr.net/npm/sweetalert2@11"></script>
    <script src="/assets/js/plugins/swiper.min.js"></script>
    <script src="/assets/js/plugins/ResizeSensor.min.js"></script>
    <script src="/assets/js/plugins/theia-sticky-sidebar.min.js"></script>
    <script src="/assets/js/plugins/wNumb.js"></script>
    <!-- JS Template -->
    <script src="/assets/js/theme.js"></script>
    <!-- Here goes your custom.js -->
    <script src="/assets/js/custom.js"></script>
    <script src="/global.js"></script>
    <partial name="Shared/_ValidationScriptsPartial" />
    @RenderSectionAsync("Scripts", false)
    <script>
        $(document).ready(function () {
            $('#countdown--offer-slider').ClassyCountdown({
                theme: "black",
                end: $.now() + 645600,
                labels: false,
            });
        });
    </script>
</body>

</html>